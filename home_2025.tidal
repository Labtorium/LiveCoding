  n "<[c4 e4 g4] [e4 g4 b4]>" # s "superpiano" # legato 0.3 # gain 0.9,
  n "{c5 g4 e4 d4}%7" # s "superpiano" # speed "1.5 0.7 1" # pan sine
]

-- 変化を加える即興風ライン
